#include<iostream>
using namespace std;
class upper
{

  public:
    upper(int *p)
   {
      cout<<*p<<endl;
      delete p;
     cout<<*p<<endl; 
     double d =3.12;
     int i =  static_cast<int>(d);
     cout<<i<<endl;
   }
};
int main()
{
    int *p;
    p = new int(25);
   upper obj(p); 
   return 0;
}
