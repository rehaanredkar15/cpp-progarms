#include <iostream>
#include <iomanip>
#include <string.h>

using namespace std;

int main() {
    int i = 4;
    double d = 4.0;
    string s = "HackerRank ";
    string h;
    int g;
    cin>>g;
    g = g + i;
    cout<<g<<endl;
    double b;
    cin>>b;
    b = b + d;
    cout<<fixed<<setprecision(1)<<b<<endl;
    getline( cin ,h);
    cout<<h;
    getline (cin ,h);
    cout<<s<<h;
}
